/* Start: Responsive CSS section */
@media (max-width: 979px) {
  .nav-collapse .dropdown-menu a {
    color: white;
  }
  .nav-collapse .dropdown-menu a:hover {
    background-color: #2E3436;
  }
  .nav-collapse .nav-header,
  .nav-collapse .dropdown-submenu > a {
    display: none;
  }
}
@media (min-width: 1200px) {
  body {
    padding-top: 70px;
  }
}
.aU5 {
    background: url(<%= asset_path 'calendar.png' %>) no-repeat scroll 0px 0px transparent;
    height: 63px;
    position: relative;
    width: 70px;
}
.aRi {
    background-color: rgb(246, 246, 246);
    border: 1px solid rgb(229, 229, 229);
    padding: 21px;
    vertical-align: top;
    width: 5%;
}
.aRh {
background: -webkit-linear-gradient(#0F3, #F00);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
    color: rgb(255, 255, 255);
    font-size: 14px;
    top: -3px;
    left: 3px;
}
.aRg {
    color: rgb(34, 34, 34);
    font-size: 160%;
    top: 17px;
    left: 3px;
}
.aRj {
    color: rgb(34, 34, 34);
    font-size: 11px;
    top: 48px;
    left: 3px;
}
.aRh, .aRg, .aRj {
    font-weight: bold;
    position: absolute;
    text-align: center;
    width: 70px;
}
a.button {
  -o-user-select:none;
  -moz-user-select:none;
  user-select:none;
}

::selection {
  background:transparent;
}
 .logo {
position: absolute;
top: 0px;
left: 40%;
}
.slidesjs-navigation {
      margin-top:3px;
    }
 a.slidesjs-next,
    a.slidesjs-previous,
    a.slidesjs-play,
    a.slidesjs-stop {
      background-image: url(<%= asset_path 'btns-next-prev.png' %>);
      background-repeat: no-repeat;
      display:block;
      width:12px;
      height:18px;
      overflow: hidden;
      text-indent: -9999px;
      float: left;
      margin-right:5px;
    }
a.slidesjs-stop {
    width: 18px;
    background-position: -41px 0px;
}
    a.slidesjs-next {
      margin-right:10px;
      background-position: -12px 0;
    }

    a:hover.slidesjs-next {
      background-position: -12px -18px;
    }

    .slidesjs-previous {
      margin-right: 5px;
      float: left;
    }

    .slidesjs-next {
      margin-right: 5px;
      float: left;
    }

    .slidesjs-pagination {
      margin: 6px 0 0;
      float: right;
      list-style: none;
    }

    .slidesjs-pagination li {
      float: left;
      margin: 0 1px;
    }

    .slidesjs-pagination li a {
      display: block;
      width: 13px;
      height: 0;
      padding-top: 13px;
      background-image: url(<%= asset_path 'pagination.png' %>);
      background-position: 0 0;
      float: left;
      overflow: hidden;
    }

    .slidesjs-pagination li a.active,
    .slidesjs-pagination li a:hover.active {
      background-position: 0 -13px
    }

    .slidesjs-pagination li a:hover {
      background-position: 0 -26px
    }
    #feedback { 
  height: 113px; 
  width: 104px; 
  position: fixed; 
  top: 50%; 
  z-index: 999;
  transform: rotate(-90deg);
  -webkit-transform: rotate(-90deg); 
  -moz-transform: rotate(-90deg); 
  -o-transform: rotate(-90deg); 
  filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=3);
}

#feedback a {  
  background: #1E2A36; 
  height: 15px; 
  width: 70px; 
  padding: 8px 16px;
  color: #fff; 
  font-size: 17px; 
  font-weight: bold; 
  text-decoration: none; 
  border-bottom: solid 1px #333;
  border-left: solid 1px #333;
  border-right: solid 1px #fff;
}

#feedback a:hover { 
  background: #161F29; 
}
